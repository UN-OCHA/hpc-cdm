{
  "version": 1,
  "projects": {
    "hpc-cdm": {
      "root": "apps/hpc-cdm",
      "sourceRoot": "apps/hpc-cdm/src",
      "projectType": "application",
      "schematics": {},
      "architect": {
        "build": {
          "builder": "@nrwl/web:webpack",
          "options": {
            "outputPath": "dist/apps/hpc-cdm",
            "index": "apps/hpc-cdm/src/index.html",
            "main": "apps/hpc-cdm/src/main.tsx",
            "polyfills": "apps/hpc-cdm/src/polyfills.ts",
            "tsConfig": "apps/hpc-cdm/tsconfig.app.json",
            "assets": [
              "apps/hpc-cdm/src/favicon.ico",
              "apps/hpc-cdm/src/assets"
            ],
            "styles": [],
            "scripts": [],
            "webpackConfig": "apps/hpc-cdm/webpack"
          },
          "configurations": {
            "dev-live": {
              "fileReplacements": [
                {
                  "replace": "apps/hpc-cdm/src/environments/environment.ts",
                  "with": "apps/hpc-cdm/src/environments/environment.dev-live.ts"
                },
                {
                  "replace": "apps/hpc-cdm/src/environments/env.json",
                  "with": ".env.json"
                }
              ]
            },
            "production": {
              "fileReplacements": [
                {
                  "replace": "apps/hpc-cdm/src/environments/environment.ts",
                  "with": "apps/hpc-cdm/src/environments/environment.prod.ts"
                }
              ],
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                }
              ]
            }
          },
          "outputs": ["{options.outputPath}"]
        },
        "serve": {
          "builder": "@nrwl/web:dev-server",
          "options": {
            "buildTarget": "hpc-cdm:build"
          },
          "configurations": {
            "dev-live": {
              "buildTarget": "hpc-cdm:build:dev-live"
            },
            "production": {
              "buildTarget": "hpc-cdm:build:production"
            }
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "apps/hpc-cdm/**/*.js",
              "apps/hpc-cdm/**/*.jsx",
              "apps/hpc-cdm/**/*.ts",
              "apps/hpc-cdm/**/*.tsx",
              "apps/hpc-cdm/src/app/components/enketo-form/enketo-core.d.ts",
              "apps/hpc-cdm/**/*.spec.ts",
              "apps/hpc-cdm/**/*.spec.tsx",
              "apps/hpc-cdm/**/*.spec.js",
              "apps/hpc-cdm/**/*.spec.jsx",
              "apps/hpc-cdm/**/*.d.ts"
            ]
          },
          "outputs": ["{options.outputFile}"]
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "apps/hpc-cdm/jest.config.js",
            "passWithNoTests": true
          },
          "outputs": ["coverage/apps/hpc-cdm"]
        }
      },
      "tags": []
    },
    "hpc-cdm-e2e": {
      "root": "apps/hpc-cdm-e2e",
      "sourceRoot": "apps/hpc-cdm-e2e/src",
      "projectType": "application",
      "architect": {
        "e2e": {
          "builder": "@nrwl/cypress:cypress",
          "options": {
            "cypressConfig": "apps/hpc-cdm-e2e/cypress.json",
            "tsConfig": "apps/hpc-cdm-e2e/tsconfig.e2e.json",
            "devServerTarget": "hpc-cdm:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "hpc-cdm:serve:production"
            }
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "apps/hpc-cdm-e2e/src/**/*.ts",
              "apps/hpc-cdm-e2e/src/**/*.js"
            ]
          },
          "outputs": ["{options.outputFile}"]
        }
      },
      "tags": [],
      "implicitDependencies": ["hpc-cdm"]
    },
    "hpc-core": {
